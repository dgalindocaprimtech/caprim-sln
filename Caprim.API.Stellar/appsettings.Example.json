{
  "ConnectionStrings": {
    "DefaultConnection": "Host=your-postgresql-host.rds.amazonaws.com;Database=your_database_name;Username=your_db_username;Password=your_secure_password;SearchPath=your_schema_name"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Cognito": {
